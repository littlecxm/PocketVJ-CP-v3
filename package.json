{
  "name": "pocketvj-v3p",
  "version": "3.5.0",
  "description": "PocketVJ",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'ts-node' --project tsconfig.server.json ./src/index.ts",
    "dev:web": "vite --mode development --port=3200",
    "build": "tsc -p tsconfig.server.json",
    "build:web": "vite build --mode production",
    "prod": "tsc -p tsconfig.server.json && node bin/index.js",
    "start": "node bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magdesign/PocketVJ-CP-v3.git"
  },
  "author": "magdesign",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/magdesign/PocketVJ-CP-v3/issues"
  },
  "homepage": "https://github.com/magdesign/PocketVJ-CP-v3",
  "dependencies": {
    "@popperjs/core": "^2.11.0",
    "@types/node": "^16.11.11",
    "bootstrap": "^5.1.3",
    "dotenv": "^10.0.0",
    "jquery": "^3.6.0",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  }
}
